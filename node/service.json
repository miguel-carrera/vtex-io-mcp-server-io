{
  "memory": 128,
  "ttl": 60,
  "timeout": 50,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "getApiDefinitions": {
      "path": "/_v/mcp_server/v0/api-definitions",
      "public": true
    },
    "getApiSpec": {
      "path": "/_v/mcp_server/v0/api-spec/:group",
      "public": true
    },
    "getApiPathSpec": {
      "path": "/_v/mcp_server/v0/api-spec/:group/:path*",
      "public": true
    },
    "executeApi": {
      "path": "/_v/mcp_server/v0/execute-api",
      "public": true
    },
    "uploadApiSpec": {
      "path": "/_v/mcp_server/v0/admin/upload-spec",
      "public": false
    }
  },
  "events": {}
}
