{
  "memory": 128,
  "ttl": 60,
  "timeout": 50,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "getApiDefinitions": {
      "path": "/_v/mcp_server/v1/api-definitions",
      "public": true
    },
    "getApiDefinitionsGroup": {
      "path": "/_v/mcp_server/v1/api-definitions/:group",
      "public": true
    },
    "getApiSpec": {
      "path": "/_v/mcp_server/v1/api-spec/:group",
      "public": true
    },
    "getApiPathSpec": {
      "path": "/_v/mcp_server/v1/api-spec/:group/:path*",
      "public": true
    },
    "executeApi": {
      "path": "/_v/mcp_server/v1/execute-api",
      "public": true
    },
    "uploadApiSpec": {
      "path": "/_v/mcp_server/v1/admin/upload-spec",
      "public": false
    },
    "mcpToolsList": {
      "path": "/_v/mcp_server/v1/mcp/tools/list",
      "public": true
    },
    "mcpToolsCall": {
      "path": "/_v/mcp_server/v1/mcp/tools/call",
      "public": true
    },
    "mcpResourcesList": {
      "path": "/_v/mcp_server/v1/mcp/resources/list",
      "public": true
    },
    "mcpResourcesRead": {
      "path": "/_v/mcp_server/v1/mcp/resources/read",
      "public": true
    },
    "mcpHandshake": {
      "path": "/_v/mcp_server/v1/mcp/handshake",
      "public": true
    },
    "mcpInitialize": {
      "path": "/_v/mcp_server/v1/mcp/initialize",
      "public": true
    },
    "mcpInitialized": {
      "path": "/_v/mcp_server/v1/mcp/notifications/initialized",
      "public": true
    },
    "mcp": {
      "path": "/_v/mcp_server/v1/mcp",
      "public": true
    },
    "mcpInstance": {
      "path": "/_v/mcp_server/v1/mcp/:instance",
      "public": true
    }
  },
  "events": {}
}
